    window.app.liveChat = {
        type: 'isl-pronto',
        load: function () {
            var script = document.createElement("script");
            script.type = "text/javascript";
            script.async = true;
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(script, s);
            script.onload = function() {
                setTimeout( function() {
                        if (ISLProntoInfo.supporters > 0) {
